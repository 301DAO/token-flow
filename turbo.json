{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["dist/**", ".next/**"],
      "dependsOn": ["^build"]
    },
    "test": {
      "outputs": ["coverage/**"],
      "dependsOn": []
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "deploy": {
      "dependsOn": ["^deploy"],
      "cache": false
    },
    "invoke": {
      "cache": false,
      "outputs": [".build/**"],
      "dependsOn": ["^invoke"]
    }
  }
}
