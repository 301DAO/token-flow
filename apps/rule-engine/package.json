{
  "name": "rule-engine",
  "version": "0.0.1",
  "scripts": {
    "build": "tsc --build",
    "start": "node -r dotenv/config dist/index.js",
    "dev": "tsc -w && nodemon -r dotenv/config -q -w dist dist/index.js",
    "clean": "rm -rf dist",
    "run:file": "node -r tsconfig-paths/register --experimental-json-modules --experimental-specifier-resolution=node --loader ts-node/esm --experimental-fetch --no-warnings $npm_config_path"

  },
  "type": "module",
  "devDependencies": {
    "@types/node": "18.7.16",
    "eslint-config-custom": "workspace:*",
    "nodemon": "^2.0.19",
    "tsconfig": "workspace:*",
    "typescript": "4.8.3"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.172.0",
    "dotenv": "^16.0.2",
    "internal-common": "workspace:*",
    "database": "workspace:*",
    "ethers-client": "workspace:*",
    "@swc/core": "^1.3.2",
    "@types/node": "18.7.16",
    "@types/prettier": "^2.7.0",
    "esbuild": "^0.15.9",
    "eslint-config-custom": "workspace:*",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "tsconfig-paths": "^4.1.0",
    "typescript": "4.8.3"
  }
}
