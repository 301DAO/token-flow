{
  "extends": "tsconfig/base.json",
  "compilerOptions": {
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "target": "es5",
    "moduleResolution": "node",
    "lib": ["es2015"],
    "rootDir": "./",
    "noImplicitAny": true,
    "esModuleInterop": true,
    "isolatedModules": false
  }
}
