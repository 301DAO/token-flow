{
  "name": "token-flow",
  "version": "0.0.1",
  "description": "crypto automation",
  "workspaces": [
    "apps/*",
    "packages/*",
    "scripts"
  ],
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "deploy": "turbo run deploy"
  },
  "devDependencies": {
    "turbo": "^1.5.3"
  },
  "engines": {
    "pnpm": ">=7.0.0",
    "node": ">=16.0.0"
  }
}
